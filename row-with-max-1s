class Solution {
    int rowWithMax1s(int arr[][], int n, int m) {
        // code here
        int ans =-1;
        int sum =0;
        for(int i =0; i<n ; i++){
            int c=0;
            for(int j:arr[i]){
                if(j==1)c++;
            }
            if(sum<c){
                sum=c;
                ans = i;
            }
        }
        return ans;
    }
}
