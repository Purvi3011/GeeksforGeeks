class Solution {
    
    public String reverse(String S){
        Stack<Character> s = new Stack<>();
        for(int i = 0 ; i < S.length() ; i++){
            s.push(S.charAt(i));
        }
        StringBuilder str = new StringBuilder("");
        while(!s.isEmpty()){
            str.append(s.pop());
        }
        S = str.toString();
        return S;
    }
}
